<!--pages/verify/verify.wxml-->
<view class='top'>
  <image src='../../assets/checked.png'></image>
  <text>注册申请已提交成功，请耐心等待审核...</text>
</view>
<view class='middle'>
  <text class='date'>注册日期： {{regInfo.date}}</text>
  <text class='company'>注册公司： {{regInfo.company}}</text>
  <text class='contact'>注册姓名： {{regInfo.contact}}</text>
  <text class='phone'>注册电话： {{regInfo.phone}}</text>
</view>
<view class='order_process'>
  <view class='process_wrap' wx:for="{{processData}}" wx:key="">
    <text class='process_name'>{{item.name}}</text>
    <view class='process'>
      <view class='process_line' style="background:{{item.start}}"></view>
      <image class='process_icon' src="{{item.icon}}"></image>
      <view class='process_line' style="background:{{item.end}}"></view>
    </view>
  </view>
</view>